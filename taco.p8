pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
--initialize

function _init()
	--variables
	scene="menu"
	x=63
	y=63
end



-->8
--main update and draw

function _update()
	if scene =="menu" then
		update_menu()
	elseif scene =="game" then
		update_game()
	end
end

function _draw()
	if scene =="menu" then
		draw_menu()
	elseif scene =="game" then
		draw_game()
	end
end
-->8
--updates

function update_menu()
	if btnp(❎) then
		scene ="game"
	end
end

function update_game()
	if btn(0) then x-=1 end
	if btn(1) then x+=1 end
	
	if btnp(❎) then
		scene="menu"
	end
end
-->8
--draws

function draw_menu()
	cls()
	map(17)
	print("press ❎ to start",30,73)
end

function draw_game()
	cls()
	map()
	spr(0,x,y,2,2)
end


