pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
player = {}
player.x = -0
player.y = 100
player.sprite = 1
player.speed = 2

function moveanim()
	player.moving = true
	player.sprite += 0
	if player.sprite > 2 then
		player.sprite = 1
	end
end

	if player.isintro == true then
		player.x += player.speed 
		moveanim()
end

function _update()
	if player.isintro == true then
		return
	end
	player.moving = false
	 if (btn(0)) then 
		player.x -= player.speed 
		if player.x < -128 then
			player.x = 128
		end
		moveanim()
	 end
	 if (btn(1)) then 
		player.x += player.speed 
		if player.x > 128 then
			player.x = 0
		end
		moveanim()
	 end
	if not player.moving then
		player.sprite = 0
	end
end

function _draw()
	cls()
	map(0,0,0,20,124,124)
 spr(player.sprite, player.x, player.y)
	-- print("x:"..player.x, 0, 0)
end


 
-->8
function getpreviouspos()
	player.xprevious = player.x
	player.yprevious = player.y
end

function iscollide()
	if (player.x == solid.x and player.y == solid.y) then
			player.x = player.xprevious
			player.y = player.yprevious
	end
end
